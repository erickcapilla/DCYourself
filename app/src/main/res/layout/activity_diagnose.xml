<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="#7378DFDF"
  tools:context=".Diagnose">

  <TextView
    android:id="@+id/textView7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:fontFamily="@font/open_sans_bold"
    android:text="Ingresa los datos que se solicitan"
    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
    android:textColor="#155959"
    android:textSize="20sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.202" />

  <ImageView
    android:id="@+id/imageView9"
    android:layout_width="106dp"
    android:layout_height="129dp"
    android:layout_marginTop="16dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/logo"
    tools:ignore="MissingConstraints" />

  <TextView
    android:id="@+id/my_label3"
    android:layout_width="425dp"
    android:layout_height="650dp"
    android:layout_marginTop="204dp"
    android:background="@drawable/borderedondo"
    android:padding="10dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <TextView
    android:id="@+id/textView"
    android:layout_width="236dp"
    android:layout_height="47dp"
    android:fontFamily="@font/open_sans_bold"
    android:text="Diagnósticar"
    android:textAlignment="center"
    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
    android:textColor="@color/white"
    android:textSize="32dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.497"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/my_label3"
    app:layout_constraintVertical_bias="0.077" />

  <Button
    android:id="@+id/button"
    android:layout_width="112dp"
    android:layout_height="56dp"
    android:layout_marginBottom="56dp"
    android:background="@drawable/botonredondo"
    android:backgroundTint="@color/white"

    android:fontFamily="@font/droid_sans_bold"
    android:onClick="atras"
    android:text="Atrás"
    android:textAppearance="@style/TextAppearance.AppCompat.Large"
    android:textColor="#28A6A6"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.2"
    app:layout_constraintStart_toStartOf="parent" />

  <Button
    android:id="@+id/button7"
    android:layout_width="112dp"
    android:layout_height="56dp"
    android:layout_marginBottom="56dp"
    android:background="@drawable/botonredondo"
    android:fontFamily="@font/droid_sans_bold"
    android:onClick="siguiente"
    android:text="Diagnósticar"
    android:textAppearance="@style/TextAppearance.AppCompat.Large"
    android:textColor="@color/white"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.775"
    app:layout_constraintStart_toStartOf="parent" />

  <EditText
    android:id="@+id/editTextTextPersonName4"
    android:layout_width="314dp"
    android:layout_height="56dp"
    android:background="@drawable/textoredblanco"
    android:ems="10"
    android:fontFamily="@font/droid_sans_bold"
    android:hint="Glucosa"
    android:inputType="textEmailAddress"
    android:paddingLeft="10dp"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.493"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.474" />

  <EditText
    android:id="@+id/editTextTextPersonName5"
    android:layout_width="314dp"
    android:layout_height="56dp"
    android:background="@drawable/textoredblanco"
    android:ems="10"
    android:fontFamily="@font/droid_sans_bold"
    android:hint="Hemoglobina"
    android:inputType="textPassword"
    android:paddingLeft="10dp"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.493"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.604" />

  <EditText
    android:id="@+id/editTextTextPersonName6"
    android:layout_width="314dp"
    android:layout_height="56dp"
    android:background="@drawable/textoredblanco"
    android:ems="10"
    android:fontFamily="@font/droid_sans_bold"
    android:hint="Insulina"
    android:inputType="textPassword"
    android:paddingLeft="10dp"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.494"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.742" />

  <ImageButton
    android:background="#00FFFFFF"
    android:id="@+id/imageView7"
    android:layout_width="50dp"
    android:layout_height="50dp"
    android:layout_marginTop="16dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.048"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/sign_out" />

  <ImageView
    android:id="@+id/imageView12"
    android:layout_width="40dp"
    android:layout_height="40dp"
    android:layout_marginTop="16dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.954"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/question" />

  <ImageView
    android:id="@+id/imageView16"
    android:layout_width="36dp"
    android:layout_height="41dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.84"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.474"
    app:srcCompat="@drawable/glucometro" />

</androidx.constraintlayout.widget.ConstraintLayout>