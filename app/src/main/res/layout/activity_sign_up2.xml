<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="#7378DFDF"
  tools:context=".SignUp2">

  <TextView
    android:id="@+id/my_label2"
    android:layout_width="431dp"
    android:layout_height="650dp"
    android:layout_marginTop="204dp"
    android:background="@drawable/borderedondo"
    android:padding="10dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <Button
    android:id="@+id/go_back"
    android:layout_width="112dp"
    android:layout_height="56dp"
    android:layout_marginBottom="40dp"
    android:background="@drawable/botonredondo"
    android:backgroundTint="@color/white"
    android:fontFamily="@font/droid_sans_bold"
    android:onClick="atras"
    android:text="Atrás"
    android:textAppearance="@style/TextAppearance.AppCompat.Large"
    android:textColor="#28A6A6"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.197"
    app:layout_constraintStart_toStartOf="parent" />

  <Button
    android:id="@+id/signUp"
    android:layout_width="112dp"
    android:layout_height="56dp"
    android:layout_marginBottom="40dp"
    android:background="@drawable/botonredondo"
    android:fontFamily="@font/droid_sans_bold"
    android:onClick="signUp"
    android:text="Registrar"
    android:textAppearance="@style/TextAppearance.AppCompat.Large"
    android:textColor="@color/white"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.775"
    app:layout_constraintStart_toStartOf="parent" />

  <TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:fontFamily="@font/open_sans_bold"
    android:text="Registro"
    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
    android:textColor="@color/white"
    android:textSize="32dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/my_label2"
    app:layout_constraintVertical_bias="0.052" />

  <ImageView
    android:id="@+id/imageView2"
    android:layout_width="333dp"
    android:layout_height="96dp"
    android:layout_marginTop="44dp"
    app:layout_constraintBottom_toTopOf="@+id/my_label2"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.234"
    app:srcCompat="@drawable/logo_secundario" />

  <EditText
    android:id="@+id/editTextLastName2"
    android:layout_width="314dp"
    android:layout_height="56dp"
    android:background="@drawable/textoredblanco"
    android:ems="10"
    android:fontFamily="@font/droid_sans_bold"
    android:hint="Confirmar contraseña"
    android:inputType="textPassword"
    android:paddingLeft="10dp"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.493"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.742" />

  <EditText
    android:id="@+id/editTextName"
    android:layout_width="314dp"
    android:layout_height="56dp"
    android:background="@drawable/textoredblanco"
    android:ems="10"
    android:fontFamily="@font/droid_sans_bold"
    android:hint="Correo electrónico"
    android:inputType="textEmailAddress"
    android:paddingLeft="10dp"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.493"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.462" />

  <EditText
    android:id="@+id/editTextLastName"
    android:layout_width="314dp"
    android:layout_height="56dp"
    android:background="@drawable/textoredblanco"
    android:ems="10"
    android:fontFamily="@font/droid_sans_bold"
    android:hint="Contraseña"
    android:inputType="textPassword"
    android:paddingLeft="10dp"
    android:textSize="16sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.493"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.604" />

  <CheckBox
    android:id="@+id/accept"
    android:layout_width="257dp"
    android:layout_height="43dp"
    android:backgroundTint="#FFFFFF"
    android:buttonTint="#FFFFFF"
    android:drawableTint="#FFFFFF"
    android:fontFamily="@font/droid_sans_bold"
    android:foregroundTint="#FFFFFF"
    android:text="Acepto términos y condiciones"
    android:textColor="#FFFFFF"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.286"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.838" />

  <ImageView
    android:id="@+id/imageView4"
    android:layout_width="56dp"
    android:layout_height="7dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.388"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.39"
    app:srcCompat="@drawable/indicator_inactive" />

  <ImageView
    android:id="@+id/imageView3"
    android:layout_width="56dp"
    android:layout_height="7dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.614"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.39"
    app:srcCompat="@drawable/indicator_active" />

</androidx.constraintlayout.widget.ConstraintLayout>